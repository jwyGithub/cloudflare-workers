{
    "name": "cloudflare-workers",
    "type": "module",
    "version": "1.0.0",
    "description": "cloudflare workers",
    "author": "jiangweiye <jiangweiye@outlook.com> (https://github.com/jwyGithub)",
    "license": "MIT",
    "homepage": "https://github.com/jwyGithub/cloudflare-workers",
    "keywords": [
        "cloudflare",
        "workers"
    ],
    "main": "index.js",
    "scripts": {
        "dev:npm": "pnpm --dir packages/npm-proxy run dev",
        "deploy:npm": "pnpm --dir packages/npm-proxy run deploy",
        "pages:npm": "pnpm --dir packages/npm-proxy run pages",
        "dev:docker": "pnpm --dir packages/docker-proxy run dev",
        "deploy:docker": "pnpm --dir packages/docker-proxy run deploy",
        "pages:docker": "pnpm --dir packages/docker-proxy run pages",
        "dev:cargo": "pnpm --dir packages/cargo-proxy run dev",
        "deploy:cargo": "pnpm --dir packages/cargo-proxy run deploy",
        "pages:cargo": "pnpm --dir packages/cargo-proxy run pages",
        "dev:download": "pnpm --dir packages/download-proxy run dev",
        "deploy:download": "pnpm --dir packages/download-proxy run deploy",
        "pages:download": "pnpm --dir packages/download-proxy run pages",
        "dev:parse": "pnpm --dir packages/vps-parse run dev",
        "deploy:parse": "pnpm --dir packages/vps-parse run deploy",
        "pages:parse": "pnpm --dir packages/vps-parse run pages"
    },
    "dependencies": {
        "cloudflare": "^3.5.0",
        "cloudflare-tools": "^0.1.1",
        "ini": "^5.0.0",
        "js-yaml": "^4.1.0"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20241127.0",
        "@jiangweiye/eslint-config": "0.1.13",
        "@jiangweiye/prettier-config": "^0.0.18",
        "@jiangweiye/tsconfig": "^0.0.11",
        "@types/ini": "^4.1.1",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^22.10.1",
        "eslint": "^9.10.0",
        "prettier": "^3.4.1",
        "typescript": "^5.7.2",
        "vite": "^6.0.1"
    }
}
