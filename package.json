{
    "name": "cloudflare-workers",
    "type": "module",
    "version": "1.0.0",
    "description": "cloudflare workers",
    "author": "jiangweiye <jiangweiye@outlook.com> (https://github.com/jwyGithub)",
    "license": "MIT",
    "homepage": "https://github.com/jwyGithub/cloudflare-workers",
    "keywords": [
        "cloudflare",
        "workers"
    ],
    "main": "index.js",
    "scripts": {
        "dev:npm": "pnpm --dir packages/npm-proxy run dev",
        "deploy:npm": "pnpm --dir packages/npm-proxy run deploy",
        "pages:npm": "pnpm --dir packages/npm-proxy run pages",
        "dev:docker": "pnpm --dir packages/docker-proxy run dev",
        "deploy:docker": "pnpm --dir packages/docker-proxy run deploy",
        "pages:docker": "pnpm --dir packages/docker-proxy run pages",
        "dev:cargo": "pnpm --dir packages/cargo-proxy run dev",
        "deploy:cargo": "pnpm --dir packages/cargo-proxy run deploy",
        "pages:cargo": "pnpm --dir packages/cargo-proxy run pages",
        "dev:vps": "pnpm --dir packages/vps-parse run dev",
        "deploy:vps": "pnpm --dir packages/vps-parse run deploy",
        "pages:vps": "pnpm --dir packages/vps-parse run pages",
        "dev:convert": "pnpm --dir packages/vps-convert run dev",
        "deploy:convert": "pnpm --dir packages/vps-convert run deploy",
        "pages:convert": "pnpm --dir packages/vps-convert run pages"
    },
    "dependencies": {
        "@jiangweiye/cloudflare-service": "^0.0.7",
        "@jiangweiye/cloudflare-shared": "^0.0.8",
        "js-yaml": "^4.1.0"
